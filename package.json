{
  "name": "@vega-studio/skeleton-kibana-plugin-dev",
  "version": "1.0.0",
  "main": "dist/lib.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "stop": "docker-compose stop",
    "kibana": "node scripts/kibana",
    "kibana-src": "node scripts/kibana-src",
    "kibana-dev": "node scripts/kibana-dev",
    "kibana-watch-plugin": "nodemon -I -w kibana-extra/diamond-chart scripts/update-plugins",
    "postinstall": "npm rebuild node-sass",
    "kbn": "node kibana/scripts/kbn"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vega-studio/hss-stacked-bar-chart.git"
  },
  "keywords": [],
  "author": "Vega",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@elastic/eslint-config-kibana": "link:kibana/packages/eslint-config-kibana",
    "@elastic/eslint-import-resolver-kibana": "link:kibana/packages/kbn-eslint-import-resolver-kibana",
    "@kbn/plugin-helpers": "link:kibana/packages/kbn-plugin-helpers",
    "@seznam/compose-react-refs": "1.0.2",
    "@tarwich/runner": "1.11.0",
    "@types/d3-color": "1.2.2",
    "@types/d3-ease": "1.0.8",
    "@types/d3-scale": "2.1.1",
    "@types/dat.gui": "0.7.2",
    "@types/fuzzy-search": "2.1.0",
    "@types/node": "11.13.5",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/shelljs": "0.8.6",
    "babel-loader": "8.0.6",
    "base64-image-loader": "1.2.1",
    "base64-inline-loader": "1.1.1",
    "circular-dependency-plugin": "5.2.0",
    "classnames": "2.2.6",
    "commander": "2.19.0",
    "compression": "1.7.4",
    "css-loader": "3.2.0",
    "d3-color": "1.2.3",
    "d3-ease": "1.0.5",
    "d3-scale": "3.0.0",
    "dat.gui": "0.7.6",
    "express": "4.17.1",
    "fibers": "4.0.1",
    "file-loader": "1.1.11",
    "fs-extra": "8.1.0",
    "fuzzy-search": "3.0.1",
    "jszip": "3.7.0",
    "mobx": "5.13.0",
    "mobx-react": "6.1.3",
    "node-sass": "4.12.0",
    "node-watch": "0.6.3",
    "nodemon": "1.19.4",
    "npm-path": "2.0.4",
    "prettier": "1.18.2",
    "ramda": "0.26.1",
    "raw-loader": "0.5.1",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "sass": "1.14.3",
    "sass-loader": "8.0.0",
    "shelljs": "0.8.3",
    "style-loader": "0.20.3",
    "ts-loader": "6.0.4",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typescript": "3.6.3",
    "webpack": "4.36.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-dev-server": "3.9.0"
  },
  "description": "",
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "not IE > 10"
  ],
  "engines": {
    "node": "10.15.2",
    "yarn": "^1.10.1"
  },
  "runner": {
    "sources": [
      {
        "name": "client",
        "entry": "test/index.html",
        "parcel": {
          "outDir": "build/client",
          "hmr": false
        }
      },
      {
        "name": "server",
        "entry": "test/server.js",
        "run": true,
        "parcel": {
          "cacheDir": "./.cache/server",
          "outDir": "./build/server",
          "outFile": "index.js",
          "target": "node",
          "minify": false,
          "hmr": false
        }
      },
      {
        "name": "lib",
        "entry": "src/index.ts",
        "parcel": {
          "outDir": "./dist/",
          "outFile": "lib.js",
          "sourceMaps": true
        }
      },
      {
        "name": "plugin",
        "entry": "src/index.ts",
        "production": true,
        "parcel": {
          "outDir": "./kibana-extra/diamond-chart/public/lib",
          "outFile": "chart.js",
          "sourceMaps": true
        }
      }
    ]
  },
  "string": [
    [
      "woff2",
      "base64",
      "data:font/woff2;charset=utf-8;base64,",
      "",
      true
    ],
    "fs"
  ]
}
